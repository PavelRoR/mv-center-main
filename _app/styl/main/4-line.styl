#grey-line
	padding 60px 0
	text-align center
	background-color: #f5f5f5
	.container
		position relative
.grey-line-img
	position absolute
	left -20%
	top -105px
	animation fr
	animation-duration: 4s
	animation-iteration-count: infinite
	
@keyframes fr
	0%
		transform: translateX(0px)translateY(0px)rotate(0deg)scale(1,1);
		animation-timing-function: linear;
	50%
		transform: translateX(0px)translateY(0px)rotate(10deg)scale(1,1);
		animation-timing-function: linear;
	100%
		transform: translateX(0px)translateY(0px)rotate(0deg)scale(1,1);