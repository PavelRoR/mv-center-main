*
	margin 0
	padding 0
	outline none
body
	font-size: 19px
	font-family: A
	font-weight: normal
	color #000
	background-color #fff
	line-height: 26px
	@media screen and (max-width 560px)
		font-size: 17px
		line-height: 23px
.container
	width 1230px
	// padding 0
	margin: 0 auto;
	position relative
	@media screen and (min-width 2295px)
		max-width 1400px
	@media screen and (max-width 1230px)
		width: 1170px
	@media screen and (max-width 1199px)
		width: 970px;
	@media screen and (max-width 991px)
		width: 750px;
	@media screen and (max-width 767px)
		width auto
.content
	position relative
	z-index 50
.wrapper
	overflow hidden
a
	color: #000
	&:focus
		outline: 0
		text-decoration none
	&:hover
		text-decoration none
h1,h2,h3,h4,h5,h6
	font-weight 500
	margin 0
button
	border none
section
header
footer
	width 100%
	display block
.video-wrapper
	margin 0 auto
	position relative
	width 100%
	top 0
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	&:after
		content: ''
		display block
		position: absolute
		top: 0
		left 0
		right 0
		width: 100%
		height 70px
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #000
		border-radius 0
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer
		margin 0 auto
.button
	display inline-block
	text-align center
	margin 0 auto
	text-decoration none
	color: #FEE7E6
	width: auto
	font-family: PH
	font-weight: normal
	background-color: #48817B;
	border-radius: 5px;
	padding 17px 74px
	transition: all .3s ease
	&:focus
		text-decoration none
	&:hover
		text-decoration none